{
  "manifest_version": 2,
  "name": "cext1",
  "version": "1.0",
  "browser_action": {
    "default_popup": "index.html"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": ["contents.js"]
    }
  ],
  "permissions": ["tabs"]
}